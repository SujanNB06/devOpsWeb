pipeline {
    agent any

    tools {
        maven 'M3'
    }

    stages {
        stage('Git Checkout') {
            steps {
                git 'https://github.com/rajuahirwar0886/devOpsWeb.git'
            }
        }

        stage('Build') {
            steps {
                sh 'mvn clean package'
            }
        }
    }
}    
